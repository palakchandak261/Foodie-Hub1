<!DOCTYPE html>
<html>
<head>
  <title>Admin Panel</title>
  <link rel="stylesheet" href="/bootstrap/css/bootstrap.min.css">
</head>
<body class="p-4">
  <div class="container">
    <h2>ðŸ‘‘ Admin Dashboard</h2>
    <table class="table">
      <thead>
        <tr><th>ID</th><th>Name</th><th>Email</th><th>Role</th><th>Rewards</th></tr>
      </thead>
      <tbody>
        <% users.forEach(u => { %>
          <tr>
            <td><%= u.user_id %></td>
            <td><%= u.name %></td>
            <td><%= u.email %></td>
            <td><%= u.role %></td>
            <td><%= u.rewards %></td>
          </tr>
        <% }) %>
      </tbody>
    </table>
  </div>
</body>
</html>
